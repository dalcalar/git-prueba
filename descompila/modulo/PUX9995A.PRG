PROCEDURE Doc_9995a
 PARAMETER dtDo, dsEr, ddOc, htDo, hsEr, hdOc
 DO CASE
      CASE LEN(ALLTRIM(dtDo))>0 .AND. dtDo<>"FA" .AND. dtDo<>"BO" .AND.  ;
           dtDo<>"FE" .AND. dtDo<>"BE" .AND. dtDo<>"FC" .AND. dtDo<>"BC"  ;
           .AND. dtDo<>"NP" .AND. dtDo<>"CO" .AND. dtDo<>"PE" .AND. dtDo<> ;
           "GR" .AND. dtDo<>"GI" .AND. dtDo<>"GS"
           STORE "Tipo de Documento no Válido..." TO mmSj
      CASE LEN(ALLTRIM(dsEr))<>0 .AND. LEN(ALLTRIM(dsEr))<>3
           STORE "Serie de Documento es de 3 dígitos..." TO mmSj
      CASE LEN(ALLTRIM(ddOc))<>0 .AND. LEN(ALLTRIM(ddOc))<>7
           STORE "Número de Documento es de 7 dígitos..." TO mmSj
      CASE LEN(ALLTRIM(htDo))>0 .AND. htDo<>"FA" .AND. htDo<>"BO" .AND.  ;
           htDo<>"FE" .AND. htDo<>"BE" .AND. htDo<>"FC" .AND. htDo<>"BC"  ;
           .AND. htDo<>"NP" .AND. htDo<>"CO" .AND. htDo<>"PE" .AND. htDo<> ;
           "GR" .AND. htDo<>"GI" .AND. htDo<>"GS"
           STORE "Tipo de Documento no Válido..." TO mmSj
      CASE LEN(ALLTRIM(hsEr))<>0 .AND. LEN(ALLTRIM(hsEr))<>3
           STORE "Serie de Documento es de 3 dígitos..." TO mmSj
      CASE LEN(ALLTRIM(hdOc))<>0 .AND. LEN(ALLTRIM(hdOc))<>7
           STORE "Número de Documento es de 7 dígitos..." TO mmSj
      CASE LEN(ALLTRIM(dsEr))=3 .AND. LEN(ALLTRIM(dtDo))=0
           STORE "Serie no se puede llenar sin el Tipo de Documento..." TO mmSj
      CASE LEN(ALLTRIM(ddOc))=7 .AND. (LEN(ALLTRIM(dtDo))=0 .OR.  ;
           LEN(ALLTRIM(dsEr))=0)
           STORE "Número no se puede llenar sin el Tipo o Serie de Documento..."  ;
                 TO mmSj
      CASE LEN(ALLTRIM(hsEr))=3 .AND. LEN(ALLTRIM(htDo))=0
           STORE "Serie no se puede llenar sin el Tipo de Documento..." TO mmSj
      CASE LEN(ALLTRIM(hdOc))=7 .AND. (LEN(ALLTRIM(htDo))=0 .OR.  ;
           LEN(ALLTRIM(hsEr))=0)
           STORE "Número no se puede llenar sin el Tipo o Serie de Documento..."  ;
                 TO mmSj
      CASE LEN(ALLTRIM(htDo))>0 .AND. LEN(ALLTRIM(dtDo+dsEr+ddOc))<12
           STORE "No se puede llenar el Tipo de Documento final sin Rango Inicial..."  ;
                 TO mmSj
      CASE LEN(ALLTRIM(hsEr))=3 .AND. LEN(ALLTRIM(dtDo+dsEr+ddOc))<12
           STORE "No se puede llenar la Serie de Documento final sin Rango Inicial..."  ;
                 TO mmSj
      CASE LEN(ALLTRIM(hdOc))=7 .AND. LEN(ALLTRIM(dtDo+dsEr+ddOc))<12
           STORE "No se puede llenar el Número de Documento final sin Rango Inicial..."  ;
                 TO mmSj
      CASE (dtDo="FA" .OR. dtDo="BO" .OR. dtDo="FE" .OR. dtDo="BE" .OR.  ;
           dtDo="FC" .OR. dtDo="BC" .OR. dtDo="NP") .AND. (htDo<>"FA"  ;
           .AND. htDo<>"BO" .AND. htDo<>"FE" .AND. htDo<>"FC" .AND. htDo<> ;
           "BE" .AND. htDo<>"BC" .AND. htDo<>"NP") .AND. LEN(ALLTRIM(htDo))>0
           STORE "Tipo de Documento de Ventas debe ser FA o BO o NP en Rangos de Documentos..."  ;
                 TO mmSj
      CASE (htDo="FA" .OR. htDo="BO" .OR. htDo="FE" .OR. htDo="BE" .OR.  ;
           htDo="FC" .OR. htDo="BC" .OR. htDo="NP") .AND. (dtDo<>"FA"  ;
           .AND. dtDo<>"BO" .AND. dtDo<>"NP") .AND. LEN(ALLTRIM(htDo))>0
           STORE "Tipo de Documento de Ventas debe ser FA o BO o NP en Rangos de Documentos..."  ;
                 TO mmSj
      CASE dtDo<>"FA" .AND. dtDo<>"BO" .AND. dtDo<>"FE" .AND. dtDo<>"BE"  ;
           .AND. dtDo<>"FC" .AND. dtDo<>"BC" .AND. dtDo<>"NP" .AND. htDo<> ;
           dtDo .AND. LEN(ALLTRIM(htDo))>0
           STORE "Tipos de Documento deben ser iguales..." TO mmSj
      CASE htDo+hsEr+hdOc<dtDo+dsEr+ddOc .AND. LEN(ALLTRIM(htDo+hsEr+hdOc))=12
           STORE "Rango Final de Documentos menor al Inicial..." TO mmSj
 ENDCASE
ENDPROC
